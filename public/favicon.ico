<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32" width="32" height="32">
  <!-- Background circle -->
  <circle cx="16" cy="16" r="16" fill="#7ED957"/>
  
  <!-- Hammer head -->
  <rect x="19" y="9" width="8" height="6" rx="1" fill="#333333"/>
  <rect x="21" y="8" width="4" height="2" rx="0.5" fill="#555555"/>
  
  <!-- Hammer handle -->
  <rect x="7" y="12" width="14" height="3" rx="1.5" fill="#8B4513" transform="rotate(-45 14 13.5)"/>
  
  <!-- Handle grip lines -->
  <line x1="10" y1="14" x2="11" y2="15" stroke="#654321" stroke-width="0.8" stroke-linecap="round"/>
  <line x1="12" y1="16" x2="13" y2="17" stroke="#654321" stroke-width="0.8" stroke-linecap="round"/>
  <line x1="14" y1="18" x2="15" y2="19" stroke="#654321" stroke-width="0.8" stroke-linecap="round"/>
  
  <!-- Metal shine effect on hammer head -->
  <rect x="20" y="10" width="1.5" height="3" rx="0.5" fill="#666666" opacity="0.5"/>
</svg>
